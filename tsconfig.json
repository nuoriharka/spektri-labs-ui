{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "es6"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
  "photos/*": ["./photos/*"],
  "@spektri/agents": ["./packages/agents/src/index.ts"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": [
    "node_modules",
    "vendor",
    "vendor/**",
    "src/components/footer.tsx",
    "src/app/page_new.tsx",
  "src/app/page_clean.tsx",
  "src/components/chart-area-interactive.tsx",
  "src/components/ui/chart.tsx",
  "src/stories/**",
  "src/lib/analytics/**",
  "src/lib/monitoring/**",
  "src/lib/store/**",
  "src/lib/websocket/**"
  ]
}
